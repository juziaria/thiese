<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.gdy.thieseback.mapper.EmployMeetingMapper">    <select id="findEmployMeeting" resultType="com.gdy.thieseback.entity.EmployMeeting" >        select * from employ_meeting where 1 = 1        <if test="time != null and time != ''">             #{beginTime}        </if>        <if test="master != null and master != ''">            and master like CONCAT('%', #{master}, '%')        </if>        <if test="major != null and major != ''">            and major like CONCAT('%', #{major}, '%')        </if>        <if test="place != null and place != '' ">            and place like CONCAT('%', #{place}, '%')        </if>        <if test="name != null and name != '' ">            and name like CONCAT('%', #{name}, '%')        </if>    </select></mapper>
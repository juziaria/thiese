<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.gdy.thieseback.mapper.EmployDoubleMeetingMapper">    <select id="findEmployPosition" resultType="com.gdy.thieseback.entity.EmployDoubleMeeting" >        select * from employ_position where 1 = 1        <if test="meetingtime != null and meetingtime != ''">             #{meetingtime}        </if>        <if test="interviewtime != null and interviewtime != ''">            #{interviewtime}        </if>        <if test="name != null and name != ''">            and name like CONCAT('%', #{name}, '%')        </if>time name master place address amount        <if test="master != null and master != ''">            and master like CONCAT('%', #{master}, '%')        </if>        <if test="place != null and place != '' ">            and place like CONCAT('%', #{place}, '%')        </if>        <if test="major != null and major != '' ">            and major like CONCAT('%', #{major}, '%')        </if>    </select></mapper>